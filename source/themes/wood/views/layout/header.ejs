
<div class="container">
    <div class="navbar">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>

          <a class="pull-right" style="margin-top:5px;" href="/rss.xml"><img src="/img/social/rss_dark.png" /></a>
          <%
            var twitter = config.plugins.twitter;
            if(twitter.display && twitter.user) { %>
            <a class="pull-right" style="margin-top:5px;" href="http://twitter.com/<%= twitter.user %>" target="_blank"><img src="/img/social/twitter_dark.png" /></a>
          <% } %>
          <%
            var gplus = config.plugins.google.gplus;
            if(gplus.display && gplus.user) { %>
            <a class="pull-right" style="margin-top:5px;" href="http://https://plus.google.com/u/0/<%= gplus.user %>" target="_blank"><img src="/img/social/google_dark.png" /></a>
          <% } %>

          <div class="nav-collapse">
            <ul class="nav">
              <li>
                <a href="/">Home</a>
              </li>
              <li>
                <a href="/archives">Archives</a>
              </li>
                <% for(var id in pages) {
                    var page = pages[id];
                    if(page.head.status == 'draft') { continue; }
                %>
                    <li>
                        <a href="<%= page._path %>"><%= page.head.title %></a>
                    </li>
                <% } %>
            </ul>
          </div>
        </div>
      </div>
    </div>
</div>